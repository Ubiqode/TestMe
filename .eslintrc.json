{
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"parser": "babel-eslint",
	"extends": "airbnb",
	"rules": {
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"],
		"react/jsx-filename-extension": 0,
		"no-tabs": 0,
		"semi": 0,
		"import/no-extraneous-dependencies": 0,
		"indent": 0,
		"arrow-parens": 0,
		"import/extensions": 0,
		"import/no-unresolved": 0,
		"react/prefer-stateless-function": 0
	}
}